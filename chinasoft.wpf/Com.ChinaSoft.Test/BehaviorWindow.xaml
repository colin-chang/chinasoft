<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Com.ChinaSoft.Test"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions" x:Class="Com.ChinaSoft.Test.BehaviorWindow"
        mc:Ignorable="d"
        Title="BehaviorWindow" Height="768" Width="1024" WindowState="Maximized">
    <Grid>
        <Rectangle Fill="#FFF4F4F5" Margin="347.458,240.125,475.977,345.807" Stroke="Black">
            <i:Interaction.Behaviors>
                <ei:TranslateZoomRotateBehavior ConstrainToParentBounds="True" SupportedGestures="All"/>
                <ei:FluidMoveBehavior/>
            </i:Interaction.Behaviors>
        </Rectangle>

    </Grid>
</Window>
